{
  "name": "My Focus Tab",
  "description": "A clean, focused new tab experience",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
"permissions": ["storage", "geolocation"],
"host_permissions": [
  "https://api.openweathermap.org/*",
  "https://api.quotable.io/*",
  "https://api.unsplash.com/*"
]
}

